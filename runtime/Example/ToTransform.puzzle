PUZZLE {
	name = "My puzzle"
	description = "Some description"
	
	INPUTS {
		int myVar = 2
		bool boolVar = true
		string stringVar = "Hello"
	}
	
	TEST {
		INPUTS {
			myVar = 5
		}
		
		EXPECTS 5
		
		ON_FAIL {
			msg = "Expected 5"
		}
	}
	
	TEST {
		INPUTS {
			boolVar = 5
		}
		
		EXPECTS false
		
		ON_FAIL {
			msg = "Expected false"
		}
	}
}