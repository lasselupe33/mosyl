      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.mdse.game.presentation.GameModelWizardID"
            name="%_UI_GameModelWizard_label"
            class="org.mdse.game.presentation.GameModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GameModelFile.gif">
