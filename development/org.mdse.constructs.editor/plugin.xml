<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated constructs -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.mdse.constructs.presentation.ConstructsModelWizardID"
            name="%_UI_ConstructsModelWizard_label"
            class="org.mdse.constructs.presentation.ConstructsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ConstructsModelFile.gif">
         <description>%_UI_ConstructsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated constructs -->
      <editor
            id="org.mdse.constructs.presentation.ConstructsEditorID"
            name="%_UI_ConstructsEditor_label"
            icon="icons/full/obj16/ConstructsModelFile.gif"
            extensions="constructs"
            class="org.mdse.constructs.presentation.ConstructsEditor"
            contributorClass="org.mdse.constructs.presentation.ConstructsActionBarContributor">
      </editor>
   </extension>

</plugin>
